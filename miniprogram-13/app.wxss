/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 


page{
    height: 100%;
    flex: 1;
    flex-direction: column;
    background: #ededed;
  }
  
  .page{
    display: flex;
    flex-direction: column;
    flex: 1;
    height: 100%;
}

.image-list{ display: flex;flex-direction: row; }

.image-list .image-item{
    flex: 1;
}

.image-list .image-item .image{
    display: block;
    width: 100%;
    max-height: 200rpx;
}


.article-item{ 
    padding: 10px 0;
    font-size: 32rpx;
    font-weight: 500;
    border-bottom: #f1f1f1 solid 2rpx;
    line-height: 40rpx;
    display: flex;
}



.article-type-base{

}

.article-type-picture{

}

.article-type-imgleft{
    display: flex;
    flex-direction: row;

}

.article-type-imgleft .article-thumb{
    width: 160rpx;
    height: 160rpx;
    
}
.article-type-imgleft .article-info{
    flex: 1;
    padding-left: 20rpx;
}

.article-type-imgright{
    display: flex;

}

.article-type-imgright .article-info{
    flex: 1;
    padding-right: 20rpx;
}

.article-type-imgright .article-thumb{
    width: 160rpx;
    height: 160rpx;

}
.article-type-imgbottom{
    display: flex;
    flex-direction: column;

}

.article-type-imgbottom .article-thumb{
    width: 100%;
    height: 320rpx;

}


.item-article{
    border-radius: 16rpx;
    background: #fff;
    padding: 32rpx;
    border-bottom: #f1f1f1 solid 2rpx;
}

.item-article .article-title{
    font-size: 36rpx;
    padding-bottom: 20rpx;
    font-weight: 500;
}

.item-article .article-quote{
    font-size: 28rpx;
    color:#666;

}

.item-article .article-date{
    font-size: 24rpx;
    color: #aaa;
    padding: 0;
}

.item-article .image-list{


}

.item-article .article-location{
    font-size: 14px;
    padding: 20rpx 0;
    color: #888;

}

.image-left-right-area{ width: 80px;height:80px;display: flex;align-items: center;flex-direction: column;background: #f5f6f5;}


.article-type-imgbottom{
    border-radius: 16rpx;background: #fff;padding: 16px;display: flex;flex-direction: column;
}

/* 默认 */
.manner_default{

}

.manner-red  .page-body , .manner-red .card-footer-menus{
    background: #f26690;
} 

.manner-red .card-tag-item , .manner-red .card-btn-phone{
    background: #3388ff;
    color: #fff;
}


/* 深色风格 */
.manner-dark{


}

.manner-dark  .page-body , .manner-red .card-footer-menus{
    background: #333;
} 

.manner-dark .card-tag-item , .manner-red .card-btn-phone{
    background: #333;
    color: #fff;
}


/* 红色风格 */
.manner-red{

}

.manner-red  .page-body , .manner-red .card-footer-menus{
    background: #f26690;
} 

.manner-red .card-tag-item , .manner-red .card-btn-phone{
    background: #f26690;
    color: #fff;
}
/* 橘色风格 */
.manner-orange{

}

.manner-orange .page-body{
    background:#f1b204
}


.no_more_data{
    display:flex;
    align-items:center;
    flex-direction:column;
    padding: 20px;
}

.load_more{
    color: #666;
    line-height: 100%;
    font-size: 28rpx;
    text-align: center;
    padding: 40rpx 0;
}

.no_more_data{

    padding: 40rpx;
    font-size: 14px;
    text-align: center;
    color: #aaa;

}



